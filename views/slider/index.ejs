<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title d-inline">Slider</h4>
            <a href="/admin/slider/create" class="btn btn-gradient-success btn-fw float-right">Add Slider</a>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th style="width:5%"> Id </th>
                        <th style="width:50%"> Img </th>
                        <th style="width:15%"> Date Create </th>
                        <th style="width:10%"> Creator </th>
                        <th style="width:20%"> </th>
                    </tr>
                </thead>
                <tbody>
                    <% sliderList.forEach((el,index) => { %>
                    <tr>
                        <td><%= index + 1 %> </td>
                        <td><img src="/img/slider/<%= el.logo %> " alt=""
                                style="width: 400px;height: 200px;border-radius: 0%;"></td>
                        <td><%=el.dateCreate.toDateString()%></td>
                        <td><%=el.creator%></td>
                        <td><a href="/admin/slider/delete/<%= el._id %>?fileName=<%=el.logo%>"
                                class="btn btn-gradient-danger  btn-fw">Delete</a></td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
